{
  "name": "js13k-rollup-starter",
  "version": "0.1.0",
  "description": "Rollup boilerplate for js13k",
  "main": "src/index.js",
  "scripts": {
    "start": "rollup -c -w",
    "build": "run-s build:*",
    "build:js": "rollup -c",
    "build:html": "type dist/index.dist.html | html-inline -b dist | html-minifier -c html-minifier.json -o dist/index.html",
    "build:zip": "7z a -mx=9  -m0=lzma -mfb=64 -md=32m dist/game.zip dist/index.dist.html dist/main.min.js dist/main.min.css",
    "build:zip-size": "node zip-size.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jose Valecillos",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "eslint": "^7.32.0",
    "html-inline": "^1.2.0",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.56.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-kontra": "^1.0.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-zip": "^1.0.2",
    "ttypescript": "^1.5.12"
  },
  "dependencies": {
    "kontra": "^7.1.3",
    "typescript": "^4.3.5"
  }
}
